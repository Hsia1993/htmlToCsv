{
  "manifest_version": 3,
  "name": "Export Table to CSV",
  "version": "1.1",
  "description": "Adds an export icon to tables to export them as CSV files",
  "permissions": ["scripting", "activeTab"],
  "host_permissions": ["<all_urls>"],
  "action": {
    "default_icon": {}
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"],
      "css": ["styles/content.css"]
    }
  ]
}
